<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE gsa-template SYSTEM "http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
    <header>
        <name>Location Repository creation</name>
        <author>Cyber Group</author>
    </header>
    <item-descriptor name="location" cache-mode="simple">
        <table name="location" type="primary" id-column-names="locationId">     
        	<property name="locationId" column-name="locationId" display-name="Location Id" writable="true">
				<attribute name="propertySortPriority" value="0"/>
			</property>       
            <property name="type" data-type="enumerated">
            	<attribute name="useCodeForValue" value="false"/>
	        	<option value="location" code="0"/>
	        	<option value="store" code="1"/>
	      	</property>
            <property name="name" data-type="string" />
            <property name="startDate" data-type="timestamp" />
            <property name="endDate" data-type="timestamp" />
            <property name="latitude" data-type="timestamp" />
            <property name="longitude" data-type="timestamp" />
            <property name="distance" data-type="timestamp" />
            <property name="address1" data-type="string" />
            <property name="address2" data-type="string" />
            <property name="address3" data-type="string" />
            <property name="city" data-type="string" />
            <property name="county" data-type="string" />
            <property name="stateAddress" data-type="string" />
            <property name="postalCode" data-type="string" />
            <property name="country" data-type="string" />
            <property name="phoneNumber" data-type="string" />
            <property name="faxNumber" data-type="string" />
            <property name="email" data-type="string" />            
        </table>
    </item-descriptor>
</gsa-template>